<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>start.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>UploadHandleServlet</servlet-name>
        <servlet-class>JPractise.UploadHandleServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>UploadHandleServlet</servlet-name>
        <url-pattern>/UploadHandleServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DownloadHandleServlet</servlet-name>
        <servlet-class>JPractise.DownloadHandleServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DownloadHandleServlet</servlet-name>
        <url-pattern>/DownloadHandleServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReadxmlByDomServlet</servlet-name>
        <servlet-class>JPractise.ReadxmlByDomServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ReadxmlByDomServlet</servlet-name>
        <url-pattern>/ReadxmlByDomServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ComputeServlet</servlet-name>
        <servlet-class>JPractise.ComputeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ComputeServlet</servlet-name>
        <url-pattern>/ComputeServlet</url-pattern>
    </servlet-mapping>
</web-app>